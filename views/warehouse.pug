doctype html
html(lang="en")
    head
        meta(charset="UTF-8")
        meta(http-equiv="X-UA-Compatible", content="IE=edge")
        meta(name="viewport", content="width=device-width, initial-scale=1.0")
        title Warehouse Information
    body 
        p City: #{warehouse.city} 
        p Street Number: #{warehouse.streetNumber} 
        p Street: #{warehouse.streetName} 
        p Country: #{(new Intl.DisplayNames(['en'], { type: 'region' })).of(warehouse.country)}
        p Province/State: #{warehouse.province}
        p Postal/ZIP Code: #{warehouse.postalCode}
